<tr id="<%= dom_id destination%>">
	<td><%= destination.id %></td>
	<td><%= destination.title %></td>
	<td><%= destination.title_cn %></td>
	<td><%= destination.created_at.localtime if destination.created_at %></td>
	<td><%= destination.updated_at.localtime if destination.updated_at %></td>
	<td><%= destination.status %></td>
	<td><%= link_to 'Show', destination %> |
	<%= link_to 'Edit', edit_admin_destination_path(destination), :remote => true %> |
	<%= link_to 'Destroy', destination, :confirm => 'Are you sure?', :method => :delete, :remote => true %></td>
</tr>